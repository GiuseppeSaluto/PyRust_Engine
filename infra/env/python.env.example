# Python API environment variables (EXAMPLE)
# Copy this file to python.env and fill with your actual values

# NASA API (Get your key at https://api.nasa.gov/)
NASA_API_KEY=YOUR_NASA_API_KEY_HERE

# Rust Engine (usa localhost per dev locale, rust-engine per Docker)
RUST_ENGINE_URL=http://localhost:8080

# MongoDB
MONGO_URI=mongodb://localhost:27017
MONGO_DB=dev

# Logging
LOG_DIRECTORY=./storage/logs

# HTTP Settings
REQUEST_TIMEOUT=30

# Flask Settings
FLASK_ENV=development
FLASK_DEBUG=true